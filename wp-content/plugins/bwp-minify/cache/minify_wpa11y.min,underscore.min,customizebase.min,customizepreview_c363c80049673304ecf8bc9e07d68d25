$('.ebp-show .single strong').html(newPrice);$('.ebp-show .total strong').html(newTotal);$('.ebp-show input[name="amount"]').val(newPrice);if($('.ebp-show span.couponResult strong').length>0){var newName=$('.ebp-show input[name="eventName"]').val()+" - coupon: "+$('.ebp-show .optCol.double span').text();$('.ebp-show input[name="item_name"]').val(newName);}
